{
    "$schema": "http://iglucentral.com/schemas/com.snowplowanalytics.self-desc/schema/jsonschema/1-0-0#",
    "self": {
        "vendor": "com.torrelabs",
        "name": "TestQuestionAnswered",
        "format": "jsonschema",
        "version": "1-0-0"
    },
    "description": "Schema for tracking single questions from the test present in the genome",
    "properties": {
        "question": {
            "description": "Question that asked by the platform",
            "type": "string",
            "maxLength": 50
        },
        "answer": {
            "description": "Answer to the question",
            "type": ["string", "null"],
            "maxLength": 50
        },
        "stepNumber": {
            "description": "Step where the question was asked",
            "type": "string",
            "maxLength": 50
        },
        "test": {
            "description": "Test that the user started: Professional culture or personality traits",
            "type": "string",
            "maxLength": 50
        },
        "sourceGgId": {
            "description": "GgId of the user that started the test",
            "type": "string",
            "maxLength": 50
        },
        "targetGgId": {
            "description": "GgId of the user that was the target of the test",
            "type": ["string", "null"],
            "maxLength": 50
        }
    },
    "additionalProperties": true,
    "type": "object",
    "required": ["sourceGgId","test", "question", "stepNumber"]
}