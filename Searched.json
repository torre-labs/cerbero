{
    "$schema": "http://iglucentral.com/schemas/com.snowplowanalytics.self-desc/schema/jsonschema/1-0-0#",
    "self": {
        "vendor": "com.torrelabs",
        "name": "Searched",
        "format": "jsonschema",
        "version": "1-0-0"
    },
    "description": "Schema for tracking the search event.",
    "properties": {
        "type": {
            "description": "Type of search made by the user: PEOPLE, JOBS, SKILLS.",
            "type": "string",
            "maxLength": 50
        },
        "query": {
            "description": "Query generated from the search made by the user.",
            "type": "string",
            "maxLength": 1000
        }
    },
    "additionalProperties": true,
    "type": "object",
    "required": ["type", "query"]
}