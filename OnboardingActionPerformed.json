{
    "$schema": "http://iglucentral.com/schemas/com.snowplowanalytics.self-desc/schema/jsonschema/1-0-0#",
    "self": {
        "vendor": "com.torrelabs",
        "name": "OnboardingActionPerformed",
        "format": "jsonschema",
        "version": "1-0-0"
    },
    "description": "Schema for tracking the actions of the user in the onboarding",
    "properties": {
        "onboarding": {
            "description": "Property of the event to describe if the user is doing the onboarding",
            "type": "boolean"
        },
        "step": {
            "description": "Step of the onboarding process",
            "type":["string","null"],
            "maxLength": 50
        }
    },
    "additionalProperties": true,
    "type": "object",
    "required": ["onboarding"]
}