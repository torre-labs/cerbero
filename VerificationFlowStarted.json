{
    "$schema": "http://iglucentral.com/schemas/com.snowplowanalytics.self-desc/schema/jsonschema/1-0-0#",
    "self": {
        "vendor": "com.torrelabs",
        "name": "VerificationFlowStarted",
        "format": "jsonschema",
        "version": "1-0-0"
    },
    "description": "Schema for contacts connected un-succesfully with torre",
    "properties": {
        "experienceId": {
            "description": "Id of the experience to be verified",
            "type": ["string", "null"],
            "maxLength": 50
        },
        "flow": {
            "description": "Flow from verification: ASK, VERIFY",
            "type": ["string", "null"],
            "maxLength": 50
        },
        "targetGgId": {
            "description": "Name of the flow where the invite is made",
            "type": ["string", "null"],
            "maxLength": 50
        }        
    },
    "additionalProperties": true,
    "type": "object",
    "required": ["sourceGgId"]
}