{
    "$schema": "http://iglucentral.com/schemas/com.snowplowanalytics.self-desc/schema/jsonschema/1-0-0#",
    "self": {
        "vendor": "com.torrelabs",
        "name": "ContactInviteAccepted",
        "format": "jsonschema",
        "version": "2-0-0"
    },
    "description": "Schema for tracking the invites that got accepted from the contacts of the user",
    "properties": {
        "inviteId": {
            "description": "Id of the invite made by the user",
            "type": ["string", "null"],
            "maxLength": 50
        },
        "medium": {
            "description": "Medium of the invitation: SMS or EMAIL",
            "type": ["string", "null"],
            "maxLength": 50
        }
    },
    "additionalProperties": true,
    "type": "object"
}
