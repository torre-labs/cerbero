{
  "$schema": "http://iglucentral.com/schemas/com.snowplowanalytics.self-desc/schema/jsonschema/1-0-0#",
  "self": {
      "vendor": "com.torrelabs",
      "name": "RedirectCrawledFlexFix",
      "format": "jsonschema",
      "version": "1-0-0"
  },
  "description": "Schema for tracking the redirection of a crawled flexible job application.",
  "properties": {
      "opportunityId": {
          "description": "Id of the opportunity the user applied",
          "type": "string",
          "maxLength": 50
      },
      "userGgid": {
          "description": "GgId of the user that applied to the job post",
          "type": ["string", "null"],
          "maxLength": 50
      }
  },
  "additionalProperties": true,
  "type": "object",
  "required": ["opportunityId"]
}
