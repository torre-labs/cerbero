{
    "$schema": "http://iglucentral.com/schemas/com.snowplowanalytics.self-desc/schema/jsonschema/1-0-0#",
    "self": {
        "vendor": "com.torrelabs",
        "name": "ContactInvited",
        "format": "jsonschema",
        "version": "1-0-0"
    },
    "description": "Schema for tracking one sigle contact invited",
    "properties": {
        "sourceUserId": {
            "description": "Id of the user that invites",
            "type": ["string", "null"],
            "maxLength": 50
        },
        "targetContactId": {
            "description": "Id of the user invited",
            "type": ["string", "null"],
            "maxLength": 50
        },
        "intent": {
            "description": "Name of the flow where the invite is made",
            "type": ["string", "null"],
            "maxLength": 50
        },
        "userGgId": {
            "description": "Ggid of the user that made the invite",
            "type": "string",
            "maxLength": 50
        }
    },
    "additionalProperties": true,
    "type": "object",
    "required": ["userGgId"]
}